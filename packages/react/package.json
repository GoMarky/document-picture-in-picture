{
    "name": "@gomarky/picture-in-picture-window-react",
    "version": "0.1.0",
    "description": "React hook for Document Picture-in-Picture API",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "picture-in-picture",
        "pip",
        "document-picture-in-picture",
        "react",
        "hook"
    ],
    "author": "gomarky",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gomarky/document-picture-in-picture.git",
        "directory": "packages/react"
    },
    "homepage": "https://github.com/gomarky/document-picture-in-picture#readme",
    "bugs": {
        "url": "https://github.com/gomarky/document-picture-in-picture/issues"
    },
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    },
    "dependencies": {
        "@gomarky/picture-in-picture-window": "workspace:*"
    },
    "devDependencies": {
        "@types/react": "^18.2.48",
        "react": "^18.2.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}